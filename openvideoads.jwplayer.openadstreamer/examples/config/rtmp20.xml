<config>
    <openadstreamer.title>
        RTMP Example 20
    </openadstreamer.title>
    <openadstreamer.json>
        {
            "allowPlaylistControl": true,

            "debug": {
                "debugger": "firebug",
                "levels": "fatal"
            },

            "ads": {
                "pauseOnClickThrough": true,
                "notice": { "textStyle": "smalltext" },
                "server": {
                    "type": "OpenX",
                    "adParameters": [ { "name": "category", "value": "food" } ],
                    "vastURL": "http://openx.openvideoads.org/openx/www/delivery/fc.php"
                },
                "schedule": [
                    {
                       "zone": "1",
                       "position": "pre-roll",
                    }
                ]
            }
        }
    </openadstreamer.json>
</config>

