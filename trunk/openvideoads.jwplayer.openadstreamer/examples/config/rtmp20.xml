<config>
    <openadstreamer.title>
        RTMP Example 20
    </openadstreamer.title>
    <openadstreamer.json>
        {
            "allowPlaylistControl": true,

            "debug": {
                "debugger": "firebug",
                "levels": "fatal"
            },

            "ads": {
                "pauseOnClickThrough": true,
                "notice": { "textStyle": "smalltext" },
                "servers": [
                    {
                       "type": "OpenX",
                       "apiAddress": "http://openx.openvideoads.org/openx/www/delivery/fc.php",
                       "customProperties": {
                           "target": "category=food"
                       }
                    }
                ],
                "schedule": [
                    {
                       "zone": "1",
                       "position": "pre-roll",
                    }
                ]
            }
        }
    </openadstreamer.json>
</config>

